[user]
    name = Jess Martin
    email = jessmartin@gmail.com
    initials = jm
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    up = !sh -c `git-up`
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[merge]
    tool = opendiff
[core]
    legacyheaders = false
[branch]
    autosetuprebase = always
[push]
    default = simple
